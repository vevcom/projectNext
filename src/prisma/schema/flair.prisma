model Flair {
  id       Int      @id @default(autoincrement())
  name     String
  cmsImage CmsImage @relation(fields: [imageId], references: [id])
  imageId  Int      @unique
  colorR   Int      @default(0)
  colorG   Int      @default(0)
  colorB   Int      @default(0)
  rank     Int      @unique @default(autoincrement())

  user User[] @relation(name: "userFlairs")
}
