{
  "name": "prismaservice",
  "version": "0.0.0",
  "description": "migrations and seeding for db",
  "type": "module",
  "scripts": {
    "build": "tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist && tsc-alias",
    "seed": "node dist/seed.js",
    "dobbelOmega-pull": "npx prisma db pull --schema ../schema.veven.prisma",
    "dobbelOmega-generate": "npx prisma generate --schema ../schema.veven.prisma"
  },
  "author": "vevcom",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.15.0",
    "rehype-format": "^5.0.0",
    "rehype-stringify": "^10.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.0",
    "sharp": "^0.33.4",
    "unified": "^11.0.4",
    "uuid": "^9.0.1"
  },
  "prisma": {
    "schema": "../schema.prisma"
  },
  "devDependencies": {
    "@types/node": "^20.14.2",
    "@types/uuid": "^9.0.8",
    "prisma": "^5.15.0",
    "tsc-alias": "^1.8.10",
    "tscpaths": "^0.0.9",
    "typescript": "^5.4.5"
  }
}
