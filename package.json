{
  "name": "projectnext",
  "author": "vevcom",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "IGNORE_SERVER_ONLY=true jest",
    "docker:dev": "./development-entrypoint.sh",
    "docker:test": "docker compose -f docker-compose.test.yml up --build --abort-on-container-exit --exit-code-from projectnext --attach projectnext --no-log-prefix",
    "docker:test:build": "docker compose --env-file .env.default -f docker-compose.prod.yml build",
    "docker:prod": "docker compose -f docker-compose.prod.yml up --build",
    "dobbelOmega:pull": "npx prisma db pull --schema ./src/prisma/vevenSchema/schema.veven.prisma",
    "dobbelOmega:generate": "npx prisma generate --schema ./src/prisma/vevenSchema/schema.veven.prisma",
    "dobbelOmega:run": "MIGRATE_FROM_VEVEN=true npm run seed",
    "docker:dobbelOmega:run": "docker compose -f docker-compose.dev.yml exec projectnext /bin/sh -c 'npm run dobbelOmega:run'",
    "docker:seed": "docker compose -f docker-compose.dev.yml exec projectnext /bin/sh -c 'npm run seed'",
    "seed": "npx prisma db push --force-reset --skip-generate && IGNORE_SERVER_ONLY=true npx prisma db seed",
    "dev-seed": "(if [ ! -d './node_modules/.prisma-dobbel-omega' ]; then npm run dobbelOmega:generate; fi) && npm run seed && npm run dev",
    "prisma-studio": "DB_URI=\"postgresql://user:password@localhost/devdb\" npx prisma studio"
  },
  "prisma": {
    "schema": "src/prisma/schema",
    "seed": "npx tsx src/prisma/seeder/seed.ts"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@prisma/client": "^6.17.1",
    "@react-email/components": "^0.5.7",
    "@react-email/render": "^1.4.0",
    "bcrypt": "^5.1.1",
    "html5-qrcode": "^2.3.8",
    "jsonwebtoken": "^9.0.2",
    "jsqr": "^1.4.0",
    "luxon": "^3.7.2",
    "next": "^15.5.6",
    "next-auth": "^4.24.11",
    "next-qrcode": "^2.5.1",
    "nodemailer": "^6.9.16",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-fontawesome": "^1.7.1",
    "react-intersection-observer": "^9.16.0",
    "react-pdf": "^9.1.1",
    "react-simplemde-editor": "^5.2.0",
    "rehype-format": "^5.0.1",
    "rehype-stringify": "^10.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "sass": "^1.93.2",
    "server-only": "^0.0.1",
    "sharp": "^0.34.4",
    "unified": "^11.0.5",
    "uuid": "^10.0.0",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.24.1",
    "zod-form-data": "^2.0.4"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.27.1",
    "@jest/globals": "^29.7.0",
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/luxon": "^3.7.1",
    "@types/node": "^22.10.2",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@types/uuid": "^10.0.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.15",
    "jest": "^29.7.0",
    "prisma": "^6.17.1",
    "ts-jest": "^29.4.5",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
