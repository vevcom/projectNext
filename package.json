{
  "name": "projectnext",
  "author": "vevcom",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "IGNORE_SERVER_ONLY=true jest",
    "docker:dev": "docker compose -f docker-compose.dev.yml up --build",
    "docker:test": "docker compose -f docker-compose.test.yml up --build --abort-on-container-exit --exit-code-from projectnext --attach projectnext --no-log-prefix",
    "docker:prod": "docker compose -f docker-compose.prod.yml up --build",
    "dobbelOmega:pull": "npx prisma db pull --schema ./src/prisma/vevenSchema/schema.veven.prisma",
    "dobbelOmega:generate": "npx prisma generate --schema ./src/prisma/vevenSchema/schema.veven.prisma",
    "dobbelOmega:run": "MIGRATE_FROM_VEVEN=true npm run seed",
    "docker:seed": "docker compose -f docker-compose.dev.yml exec projectnext /bin/sh -c 'npm run seed'",
    "seed": "npx prisma db push --force-reset --skip-generate && IGNORE_SERVER_ONLY=true npx prisma db seed",
    "dev-seed": "(if [ ! -d './node_modules/.prisma-dobbel-omega' ]; then npm run dobbelOmega-generate; fi) && npm run seed && npm run dev"
  },
  "prisma": {
    "schema": "src/prisma/schema",
    "seed": "npx tsx src/prisma/seeder/seed.ts"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@prisma/client": "^6.1.0",
    "@react-email/components": "^0.0.31",
    "@react-email/render": "^1.0.3",
    "bcrypt": "^6.0.0",
    "html5-qrcode": "^2.3.8",
    "jsonwebtoken": "^9.0.2",
    "jsqr": "^1.4.0",
    "luxon": "^3.5.0",
    "next": "^15.3.1",
    "next-auth": "^4.24.11",
    "next-qrcode": "^2.5.1",
    "nodemailer": "^7.0.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-fontawesome": "^1.7.1",
    "react-intersection-observer": "^9.14.0",
    "react-pdf": "^10.0.1",
    "react-simplemde-editor": "^5.2.0",
    "rehype-format": "^5.0.1",
    "rehype-stringify": "^10.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.1",
    "sass": "^1.83.0",
    "server-only": "^0.0.1",
    "sharp": "^0.33.5",
    "unified": "^11.0.5",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^4.0.5",
    "zod-form-data": "^3.0.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.27.1",
    "@jest/globals": "^30.0.4",
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/luxon": "^3.4.2",
    "@types/node": "^24.0.13",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/uuid": "^10.0.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.3.5",
    "jest": "^30.0.4",
    "prisma": "^6.7.0",
    "ts-jest": "^29.3.2",
    "tsx": "^4.19.3",
    "typescript": "^5.7.2"
  }
}
