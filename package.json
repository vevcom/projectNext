{
  "name": "projectnext",
  "author": "vevcom",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "IGNORE_SERVER_ONLY=true jest",
    "docker:dev": "docker compose -f docker-compose.dev.yml up --build",
    "docker:test": "docker compose -f docker-compose.test.yml up --build --abort-on-container-exit --exit-code-from projectnext --attach projectnext --no-log-prefix",
    "docker:test:build": "docker compose --env-file .env.default -f docker-compose.prod.yml build",
    "docker:prod": "docker compose -f docker-compose.prod.yml up --build",
    "dobbelOmega:pull": "npx prisma db pull --schema ./src/prisma/vevenSchema/schema.veven.prisma",
    "dobbelOmega:generate": "npx prisma generate --schema ./src/prisma/vevenSchema/schema.veven.prisma",
    "dobbelOmega:run": "MIGRATE_FROM_VEVEN=true npm run seed",
    "docker:dobbelOmega:run": "docker compose -f docker-compose.dev.yml exec projectnext /bin/sh -c 'npm run dobbelOmega:run'",
    "docker:seed": "docker compose -f docker-compose.dev.yml exec projectnext /bin/sh -c 'npm run seed'",
    "seed": "npx prisma db push --force-reset --skip-generate && IGNORE_SERVER_ONLY=true npx prisma db seed",
    "dev-seed": "(if [ ! -d './node_modules/.prisma-dobbel-omega' ]; then npm run dobbelOmega-generate; fi) && npm run seed && npm run dev"
  },
  "prisma": {
    "schema": "src/prisma/schema",
    "seed": "npx tsx src/prisma/seeder/seed.ts"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^7.0.1",
    "@fortawesome/free-brands-svg-icons": "^7.0.1",
    "@fortawesome/free-solid-svg-icons": "^7.0.1",
    "@fortawesome/react-fontawesome": "^3.0.2",
    "@prisma/client": "^6.16.2",
    "@react-email/components": "^0.5.5",
    "@react-email/render": "^1.3.1",
    "bcrypt": "^6.0.0",
    "html5-qrcode": "^2.3.8",
    "jsonwebtoken": "^9.0.2",
    "jsqr": "^1.4.0",
    "luxon": "^3.7.2",
    "next": "^15.5.4",
    "next-auth": "^4.24.11",
    "next-qrcode": "^2.5.1",
    "nodemailer": "^7.0.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-fontawesome": "^1.7.1",
    "react-intersection-observer": "^9.16.0",
    "react-pdf": "^10.1.0",
    "react-simplemde-editor": "^5.2.0",
    "rehype-format": "^5.0.1",
    "rehype-stringify": "^10.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "sass": "^1.93.2",
    "server-only": "^0.0.1",
    "sharp": "^0.34.4",
    "unified": "^11.0.5",
    "uuid": "^13.0.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^4.1.11",
    "zod-form-data": "^3.0.1"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.27.1",
    "@jest/globals": "^30.1.2",
    "@types/bcrypt": "^6.0.0",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/luxon": "^3.7.1",
    "@types/node": "^24.5.2",
    "@types/nodemailer": "^7.0.1",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@types/uuid": "^11.0.0",
    "eslint": "^9.36.0",
    "eslint-config-next": "^15.5.4",
    "jest": "^30.1.3",
    "prisma": "^6.16.2",
    "ts-jest": "^29.4.4",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
